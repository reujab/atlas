{
  "name": "pi-es",
  "version": "0.0.0",
  "description": "Pi Entertainment System",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "dev": "webpack serve -c webpack.dev & tailwindcss -o dist/index.css -w & cargo tauri dev & wait",
    "build": "webpack build -c webpack.prod & tailwindcss -o dist/index.css -m & wait && cargo build --release",
    "start": "npm i && npm run build && ./target/release/pi-es"
  },
