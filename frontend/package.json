  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/reujab/pi-es/issues"
  },
  "homepage": "https://github.com/reujab/pi-es#readme",
  "devDependencies": {
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "^8.10.0",
    "html-minimizer-webpack-plugin": "^3.5.0",
    "svelte": "^3.46.4",
    "svelte-loader": "^3.1.2",
    "tailwindcss": "^3.0.23",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-rc.2",
    "gamepad.js": "^1.0.4",
